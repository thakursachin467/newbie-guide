<style media="screen">
#map {
margin:0 auto;
width: 100%;
height: 650px;
}
</style>
<hr>
<div class="row">
  <div class="col s4">
    <nav class="search-title-filter">
      <div class="nav-wrapper">
        <form>
          <div class="input-field">
            <input id="search" name="search" type="search">
            <label for="search"><i class="material-icons">search</i></label>
          </div>
        </form>
      </div>
    </nav>
    {{#if data}}
    <table class="striped">
      <thead>
        <th>
          Name
        </th>
        <th>

        </th>
        <th>

        </th>
        <th>

            location
        </th>
      </thead>
      <tbody>
        {{#each data}}
        <tr>
          <td> <span > {{place}} </span> </td>
          <td></td>
          <td></td>
          <td> <span class="status">{{location}} {{floor}}</span> </td>
        </tr>
        {{/each}}
        </tbody>
        </table>
      {{else}}
      <table class="striped">

        <tbody>
          <tr>
            <td colspan="4"> <span >No data found</span> </td>

          </tr>
        </tbody>
        </table>
      {{/if}}
  </div>
  <div class="col s8">
    <div id="map"></div>

  </div>

</div>
<script type="text/javascript" src="../assests/js/map.js"></script>
<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDtO7ZP74l7NLZvLu_86FfW3QB0tVBJVo8&libraries=places&callback=initMap"
        onerror="googleError()" async defer></script>
